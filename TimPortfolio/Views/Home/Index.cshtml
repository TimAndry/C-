          <button href="#projects" class="pbtn2" id="plink2">PROJECTS</button>
        </div>
      </div>
      </div>
  </div>

  <div id="projects">
      <div>
        <h1 id="headline3" class=" mast mast__header mast__title">projects</h1> 
        <div id="stacks" style="display: none;">
          <h3 id="s1">PYTHON</h3>
          <h3 id="s2">MEAN</h3>
          <h3 id="s3">ASP.NET</h3>
        </div>
        <div class="projects" id="p1" style="display:none">
          <h2 id="pid">PYTHON</h2>
          <div class="project1" id="pj1">
            <img src="~/images/traintracker.png">
          </div>
          <div class="github1" id="gh1">
            <img src="~/images/traintracker.png">
          </div>
        </div>
        <div class="projects" id="p2" style="display:none">
          <h2 id="mid">MEAN</h2>
          <div class="project1" id="pj2">
            <img src="~/images/traintracker.png">
          </div>
          <div class="github1" id="gh2">
            <img src="~/images/traintracker.png">
          </div>
        </div>
        <div class="projects" id="p3" style="display:none;">
          <h2 id="aid">ASP.NET</h2>
          <div class="project1" id="pj3">
            <img src="~/images/traintracker.png">
          </div>
          <div class="github1" id="gh3">
            <img src="~/images/traintracker.png">
          </div>
        </div>
      </div>  
  </div>
  <div id="contact">
    <h1>CONTACT</h1>
    <div id="circle" class="bounce">
      
    </div>
    <div id="circle2" >
      <div id="iris" class="bouncelite">  
        
      </div>
    </div>
    <form asp-action="contact" asp-controller="Home" method="post">
      <div id="fname">
        <label asp-for="FirstName"></label>
        <input asp-for="FirstName"/>
        <span asp-validation-for="FirstName" class="error"></span>
      </div>
      <div id="lname">
        <label asp-for="LastName"></label>
        <input asp-for="LastName">
        <span asp-validation-for="LastName" class="error"></span>
      </div>
      <div id="email">
        <label asp-for="Email"></label>
        <input type="email" asp-for="Email">
        <span asp-validation-for="Email" class="error"></span>
      </div>
      <div id="comment">
        <label asp-for="Comment"></label>
        <textarea cols="35" rows="7" asp-for="Comment"></textarea>
        <span asp-validation-for="Email" class="error"></span>
      </div>
      <button type="submit" id="submit" class="pbtn3">CONTACT</button>
    </form>
    <button id="contactbtn" class="pbtn3 avatar">CONTACT ME</button>
  </div>





